{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "easycorp/easyadmin-bundle": "^3.5",
        "friendsofsymfony/rest-bundle": "^3.1",
        "jms/serializer-bundle": "^4.0",
        "lexik/jwt-authentication-bundle": "^2.14",
        "nelmio/api-doc-bundle": "^4.6",
        "pagerfanta/pagerfanta": "^3.5",
        "sensio/framework-extra-bundle": "^6.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-server-bundle": "4.4",
        "symfony/yaml": "5.4.*",
        "willdurand/hateoas": "^3.7",
        "willdurand/hateoas-bundle": "^2.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "process-timeout":0
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "create-db": "php bin/console doctrine:database:create",
        "migrate-db": "php bin/console doctrine:migrations:migrate",
        "fixtures-load": "php bin/console doctrine:fixtures:load -n",
        "create-jwt": "php bin/console lexik:jwt:generate-keypair --skip-if-exists",
        "prepare-db": [
            "@create-db",
            "@migrate-db",
            "@fixtures-load",
            "@create-jwt"
        ],
        "run-server": "php bin/console server:run",
        "stan": "php vendor/bin/phpstan analyse --memory-limit=4000M --ansi src/",
        "twig": "bin/console lint:twig templates/ --ansi",
        "csfixer": "php tools/php-cs-fixer/vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --verbose --ansi",
        "linter": [
            "@csfixer",
            "@stan"
        ],
        "bdi": "vendor/bin/bdi detect drivers",
        "test-unit": "vendor/bin/phpunit --testsuite 'Unit'",
        "test-functional": "vendor/bin/phpunit --testsuite 'Functional'",
        "test-behat": "vendor/bin/behat",
        "tests": [
          "@bdi",
          "@test-unit",
          "@test-functional",
          "@test-behat"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^6.0"
    }
}
